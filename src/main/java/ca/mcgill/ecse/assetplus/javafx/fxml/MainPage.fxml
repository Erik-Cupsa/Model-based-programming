<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" prefHeight="518.0" prefWidth="851.0">
    <TabPane tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
        <tabs>
            <Tab text="Start/Complete work">
                <content>
                    <fx:include source="pages/StartCompleteWorkOnMaintenanceTicket.fxml" />
                </content>
            </Tab>
            <Tab text="Approve/disapprove work">
                <content>
                    <fx:include source="pages/ApproveDisapproveOnMaintenanceTicket.fxml" />
                </content>
            </Tab>
            <Tab text="View Tickets">
                <content>
                    <fx:include source="pages/ViewStatusPage.fxml" />
                </content>
            </Tab>
            <Tab text="Manage Employees">
                <content>
                    <fx:include source="pages/AddUpdateDeleteEmployee.fxml" />
                </content>
            </Tab>
        </tabs>
    </TabPane>
</VBox>
